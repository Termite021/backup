
public class SmartPhone extends Mp3Player {

	public void call(){
		System.out.println("call");
	}
	public void mail(){
		System.out.println("mail");
	}
	public void photo(){
		System.out.println("photo");
	}
	public void internet(){
		System.out.println("internet");
	}

	public void play(){
		System.out.println("play(High-Resolution Audio)");
	}
	public void stop(){
		System.out.println("stop(High-Resolution Audio)");
	}
	public void next(){
		System.out.println("next(High-Resolution Audio)");
	}
	public void back(){
		System.out.println("back(High-Resolution Audio)");
	}



}
